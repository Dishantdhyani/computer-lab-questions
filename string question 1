// Online C compiler to run C program online
#include <stdio.h>

int main() {
    char s[20],i;
    int countv=0,countc=0;
    printf("enter the string");
    fgets(s,20,stdin);
    while(s[i]!='\0')
    {
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u'||s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U')
        {
            countv++;
        }
        else
        {
            countc++;
        }
        i++;
    }
    printf("%d\n",countv);
    printf("%d",countc);
    return 0;
}
